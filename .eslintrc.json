{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "standard"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "vue"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ]
    }
}
